{
  "name": "dollar-back",
  "version": "1.0.0",
  "description": "Node.js Fastify backend for DollarAlert - USD/BOB exchange rate monitoring and scraping",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "tsx watch src/main/index.ts",
    "build": "tsc",
    "start": "node dist/main/index.js",
    "lint": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "migration:push": "supabase db push --linked"
  },
  "keywords": [
    "fastify",
    "typescript",
    "scraping",
    "telegram",
    "supabase",
    "exchange-rates"
  ],
  "author": "DollarAlert Team",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.2.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/schedule": "^4.1.1",
    "@supabase/supabase-js": "^2.38.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cheerio": "^1.0.0-rc.12",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.3",
    "puppeteer-core": "^21.5.2",
    "telegraf": "^4.15.1",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.0",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supabase": "^1.115.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
