    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.3",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/ui": "^4.0.18",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "happy-dom": "^20.3.7",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^5.0.0",
