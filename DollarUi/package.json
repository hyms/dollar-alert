{
  "name": "dollar-ui",
  "version": "1.0.0",
  "description": "Vue.js 3 frontend for DollarAlert - USD/BOB exchange rate monitoring",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "type-check": "tsc --noEmit",
